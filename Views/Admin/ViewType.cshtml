@{
    ViewBag.Title = "Index";

    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}
<head>
    <title>
        admin panel
    </title>


</head>




<body style=" background-color:peachpuff">


    <div>

        <img src="~/content/themes/base/images/tab-left.gif" alt=" " width=" 1380" height="150" />
       
        <div>
                   <div class="box-body">
                    <table id="example2" class="table table-bordered table-hover">
                        <thead style=" background-color:gray">
                            <tr>
                                <th>ID</th>
                                <th>Path></th>
                                <th>Description</th>
                                <th>Category </th>
                                <th>Type </th>
                                <th>Update </th>
                                <th>Delete</th>
                                @*<th>Search </th>*@


                            </tr>

                        </thead>


                        @foreach (fp.Models.Image i in Model)
                        {
                             <tr>

                                <td>
                                    @i.Id
                                </td>
                                <td>
                                    @i.Path
                                </td>
                                <td>
                                    @i.Description
                                </td>
                                <td>
                                    @i.Category
                                </td>
                                 <td>
                                     @i.Type
                                 </td>
                                 <td>
                                     @Html.ActionLink("Update", "UpdateType", new { id = i.Id })
                                 </td>
                                 <td>
                                     @Html.ActionLink("Delete", "Delete2", new { id = i.Id })
                                 </td>
                                 </tr>
                        }
                               
                        

                    </table>
               </div>         
                       
        </div>
    </div>
</body>