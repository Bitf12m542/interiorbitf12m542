@{
    ViewBag.Title = "Comments";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using System.Collections
@using fp.Models
@model List<User>
<body>

    <div class="page">
        <div class="project-page">

            <div class="body">
                <div class="content-project">
                    <div>
                        <h3>Living Area furniture and description </h3>
                    </div>
                    <div class="navigation">
                        <span>Select any Category:</span>
                        <ul>
                            <li class="selected">
                                <a href="#">All</a>
                            </li>
                            <li>
                                <a href="/Project/living_room">Living Room</a>
                            </li>
                            <li>
                                <a href="/Project/dining_room">Dining Room</a>
                            </li>
                            <li>
                                <a href="/Project/toned_bedroom">Bedroom</a>
                            </li>
                            <li>
                                <a href="/Project/bathroom">Bathroom</a>
                            </li>
                            <li>
                                <a href="/Project/Comments">Comments</a>
                            </li>
                        </ul>
                    </div>
                    </div>
                </div>
            </div>
        </div>
   
    <center>
        
        <table border="1">
            
            @foreach (var c in Model)
            {
                <tr><td>@c.Name</td> <td>@c.Comment</td></tr> 
            }
        </table>
    </center>

        </body>
