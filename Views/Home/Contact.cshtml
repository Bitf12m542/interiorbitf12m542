    ViewBag.Title = "Contact";
    
    Layout = "~/Views/Shared/_Layout.cshtml";
   


}
<head>

    <title></title>

    <link href="bootstrap.css" rel="stylesheet" />
    <script src="bootstrap.js"> </script>
    <link rel="stylesheet" href="css/style.css" type="text/css">


</head>
<body>
    <div id="background-green2">
        background
    </div>
    <div class="page">
        <div class="contact-page">
            
            <div class="body">
                <div class="content-contact">
                    <div>
                        <div>
                            <h3>Contact</h3>
                            <p>
                                So you are Building your Home or Designing your Office and have no idea what to do, where to start or what direction you should go? You are not alone! We are here to help.

                                Building a home or office can be exhausting and if you don't have the skills and abilities in your back pocket you need some professional help. Interior & Architecture is a professional interior designing and architecture firm.To subscribe or get information about your subscription to Interior Design, see Subscriber Services, call 800-900-0804, or e-mail subscriptions@interiordesign. 
                            </p>
                            <form action="ContactUser" method="post">
                            @*<form id="Contact" action="ContactUser" method="post">*@
                                <h4>Send Us A Quick Message</h4>
                               
                                 <input type="text" value="Name *" id=" name" name ="name"onblur="this.value=!this.value?'Name *':this.value;" onfocus="this.select()" onclick="this.value='';">
                                <br />
                                <input type="text" value="Email Address *"id=" email" name ="email"onblur="this.value=!this.value?'Email Address *':this.value;" onfocus="this.select()" onclick="this.value='';">
                                <br />
                                <input type="text" value="Phone Number"id=" phone_number"name="phone_number" onblur="this.value=!this.value?'Phone Number':this.value;" onfocus="this.select()" onclick="this.value='';">
                                <br/>
                                <textarea name="comment" id="comment"name="comment" cols="30" rows="10" onblur="this.value=!this.value?'Your Comment':this.value;" onfocus="this.select()" onclick="this.value='';">Your Comment</textarea>
                                @*< id="submit" class="submit" value="Submit Message">*@
                                <button id="submit">Submit message</button>

                            </form>
                        </div>
